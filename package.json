{
  "name": "hacker-rank",
  "version": "1.0.0",
  "description": "Submissions of HackerRank problems written in TypeScript",
  "main": "index.js",
  "author": "somallg",
  "license": "MIT",
  "scripts": {
    "cz": "git-cz",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "gulp compile --f",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "test": "jest --colors --maxWorkers=2",
    "test:f": "jest --colors --runTestsByPath",
    "lint": "tslint -p tsconfig.json -c tslint.json -t stylish",
    "gulp:lint": "gulp lint",
    "enforce": "gulp enforce",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage false",
    "gulp:debug": "node --inspect node_modules/.bin/gulp",
    "gen:files": "schematics @schematics/challenges:generate-template-files",
    "fix:spec": "schematics @schematics/challenges:fix-spec-file",
    "fix:fixture": "schematics @schematics/challenges:fix-test-fixture",
    "coverage": "browser-sync start --s --ss packages/coverage/lcov-report"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/condition-travis"
    ],
    "getLastRelease": "@semantic-release/git",
    "publish": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.6.8",
    "@commitlint/cli": "5.2.8",
    "@commitlint/config-angular": "^6.0.2",
    "@semantic-release/changelog": "1.0.0",
    "@semantic-release/git": "2.0.1",
    "@types/del": "^3.0.1",
    "@types/gulp": "^3",
    "@types/gulp-load-plugins": "^0.0.31",
    "@types/jest": "^22.2.3",
    "@types/jest-matchers": "^20.0.1",
    "@types/json5": "^0.0.30",
    "@types/puppeteer": "^1.3.2",
    "@types/underscore": "^1.8.8",
    "@types/yargs": "^11.0.0",
    "bignumber.js": "^7.2.1",
    "browser-sync": "^2.26.3",
    "chalk": "2.3.0",
    "codacy-coverage": "^3.0.0",
    "coffeescript": "^2.3.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "del": "^3.0.0",
    "gulp": "3.9.1",
    "gulp-if": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-prettier-plugin": "^1.2.0",
    "gulp-print": "2.0.1",
    "gulp-task-listing": "^1.0.1",
    "gulp-tslint": "^8.1.3",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-matchers": "^20.0.3",
    "json5": "^2.0.1",
    "lerna": "^2.10.1",
    "lint-staged": "^7.1.2",
    "prettier": "^1.13.7",
    "puppeteer": "^1.3.0",
    "rollup": "^0.58.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "semantic-release": "11.0.2",
    "semantic-release-cli": "3.5.0",
    "standard-version": "4.3.0",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.1",
    "underscore": "^1.9.1",
    "yargs": "10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/somallg/hacker-rank.git"
  }
}
