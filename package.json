{
  "name": "hacker-rank",
  "version": "0.0.0",
  "description": "Submissions of HackerRank problems written in TypeScript",
  "main": "index.js",
  "author": "somallg",
  "license": "MIT",
  "scripts": {
    "cz": "git-cz",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepush": "yarn test",
    "compile": "yarn gulp compile",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "test": "yarn gulp test",
    "enforce": "yarn gulp enforce",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "gulp": "babel-node ./node_modules/.bin/gulp"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/condition-travis"
    ],
    "getLastRelease": "@semantic-release/git",
    "publish": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "5.2.8",
    "@commitlint/config-angular": "^6.0.2",
    "@semantic-release/changelog": "1.0.0",
    "@semantic-release/git": "2.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "chalk": "2.3.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "gulp": "3.9.1",
    "gulp-eslint": "^4.0.2",
    "gulp-file": "^0.4.0",
    "gulp-if": "2.0.2",
    "gulp-jest": "^3.0.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-prettier-plugin": "^1.2.0",
    "gulp-print": "2.0.1",
    "gulp-task-listing": "^1.0.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "semantic-release": "11.0.2",
    "semantic-release-cli": "3.5.0",
    "standard-version": "4.3.0",
    "tern-jasmine": "^0.3.0",
    "tern-jasminematchers": "^0.1.7",
    "yargs": "10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/somallg/hacker-rank.git"
  }
}
